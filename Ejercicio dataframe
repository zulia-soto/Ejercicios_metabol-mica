#Ejercicio Dataframe

Motivo_de_muerte <- c("Falla sistema urinario","Neoplasia","Infeccioso","Trauma",
            "Falla sistema cardiovascular","Muerte súbita","Cuerpo extraño")
A <- c(16,13,23,19,4,3,1)
B <- c(33,23,15,5,6,3,5)
C <- c(47,50,13,3,9,1,0)
D <- c(43,37,2,1,2,0,0)

Tabla <- data.frame(motivo_de_muerte=Motivo_de_muerte,"0 a 4 años"=A,
                    "5 a 9 años"=B,"10 a 14 años"=C,"15 años o más"=D)
head(Tabla)
View(Tabla)
